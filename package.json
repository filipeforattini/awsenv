{
  "name": "@vitta-health/awsenv",
  "version": "1.2.4",
  "description": "Secure way to handle environment variables in Docker with AWS Parameter Store",
  "main": "./index.js",
  "type": "module",
  "bin": {
    "awsenv": "./index.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/developers-vitta/awsenv.git"
  },
  "author": "Gilson F. B. Souza <gilsonfernandesbatista@gmail.com> (https://github.com/gilsonsouzah)",
  "contributors": [
    "Filipe Forattini <filipeforattini1@gmail.com> (https://github.com/filipeforattini/)"
  ],
  "bugs": {
    "url": "https://github.com/developers-vitta/awsenv/issues"
  },
  "scripts": {
    "build": "node scripts/build-all.js",
    "build:npm": "node scripts/build-dist.js",
    "build:binaries": "node scripts/build-binaries.js",
    "build:optimized": "node scripts/build-optimized.js",
    "build:rollup": "node scripts/build-with-rollup.js",
    "build:babel": "node scripts/build-with-babel.js",
    "build:wasm": "node scripts/build-wasm.js",
    "build:pkg": "pkg . --out-path ./releases && chmod +x ./releases/*",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "dev": "node --experimental-modules src/index.js",
    "clean": "rm -rf node_modules coverage releases",
    "sync-example": "node --experimental-modules src/index.js --sync .env.example --namespace /test/sync --dry-run"
  },
  "packageManager": "pnpm@8.0.0",
  "vitest": {
    "include": [
      "tests/**/*.test.js"
    ],
    "coverage": {
      "provider": "v8",
      "reporter": [
        "text",
        "html",
        "clover",
        "json"
      ],
      "include": [
        "src/**/*.js"
      ],
      "exclude": [
        "tests/**/*.test.js",
        "tests/**/*.spec.js"
      ],
      "thresholds": {
        "statements": 60,
        "branches": 75,
        "functions": 70,
        "lines": 60
      }
    }
  },
  "pkg": {
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "c8": "^10.1.3",
    "esbuild": "^0.25.9",
    "pkg": "^5.8.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.864.0",
    "args": "^5.0.3",
    "p-limit": "^6.2.0"
  },
  "keywords": [
    "aws",
    "env",
    "awsenv",
    "ssm",
    "docker"
  ]
}
